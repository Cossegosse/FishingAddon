{
  "schemaVersion": 1,
  "id": "fishingaddon",
  "version": "1.0.0",
  "name": "FishingAddon",
  "description": "Needed for addon loading in development.",
  "authors": ["QuiteBoring", "Twiston", "oblongboot"],
  "contact": {
    "homepage": "https://cobalt.quiteboring.dev/",
    "sources": "https://github.com/CobaltScripts/Cobalt"
  },
  "license": "CC0-1.0",
  "icon": "assets/fishingaddon/icon.png",
  "environment": "*",
  "entrypoints": {
    "cobalt": [
      {
        "adapter": "kotlin",
        "value": "com.FishingAddon.FishingAddon"
      }
    ]
  },
  "mixins": ["fishingaddon.mixins.json"],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-api": "*",
    "minecraft": "~${minecraft_version}",
    "java": ">=21",
    "fabric-language-kotlin": ">=${fabric_kotlin_version}"
  }
}
